/*MI PRIMER STORED PROCEDURE*/

CREATE OR REPLACE PROCEDURE hola_mundo
AS
MENSAJITO VARCHAR2(120);
BEGIN
MENSAJITO:='MIS PRIMEROS PROYECTOS CON LOS PROCEDIMIENTOS';
DBMS_OUTPUT.PUT_LINE(MENSAJITO);
END;
/

/*PARA PODER PROBAR UN PROCEDIMIENTO AL NO TENER UNA APLICACION QUE LO USE, PODEMOS CREAR UN BLOQUE PL/SQL*/

BEGIN
HOLA_MUNDO();
END;
